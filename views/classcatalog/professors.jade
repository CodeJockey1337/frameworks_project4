extends ../layout2

block content
    img.rounded(src='/img/MSUD_header.jpg')
    .page-header
        h2 MSUD Class Catalog - Professors
    p
    .container
        h2 Currently Employed Professors
        table.table.table-striped
            thead
                tr
                    td Professor Name
            tbody
                for professor in professors
                    tr
                        td= professor.first_name + ' ' + professor.last_name + ' - ' + professor.department